{
  "name": "next-type-safe-pages",
  "version": "0.0.5",
  "description": "Generating TypeScript types to represent all pages in a Next.js project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "bin/cli.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@types/node": "^14.14.34",
    "walk-sync": "^2.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "commander": "^7.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckastbjerg/next-type-safe-routes.git"
  },
  "keywords": [],
  "author": "Christian Kastbjerg",
  "license": "ISC"
}
